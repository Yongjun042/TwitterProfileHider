{

    "manifest_version": 2,
    "name": "TwitterPrifileHider",
    "version": "1.0",

    "description": "Hide Profile",

    "icons": {
        "48": "icons/border-48.png"
    },

    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["hide.js"]
    },

    "content_scripts": [{
        "matches": ["*://*.twitter.com/*"]
    }]

}